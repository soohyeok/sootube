extends layouts/main.pug
include mixins/videoCatalog

block content
    .search__header
        h3 Searching by #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each vid in videos
            +videoCatalog({
                title: vid.title,
                views: vid.views,
                videoFile: vid.videoFile,
                id: vid.id
            })